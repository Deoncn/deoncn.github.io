<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Deon;s blog</title>
 <link href="http://deoncn.top/atom.xml" rel="self"/>
 <link href="http://deoncn.top/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://deoncn.top/</id>
 <author>
   <name>Deon</name>
   <email>2210633536@qq.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://deoncn.top/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://deoncn.top/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
